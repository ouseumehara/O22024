{
    "sass.enabled": false,
    "path": {
        "scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "includePaths": [],
                "sourceComments": false,
                "outputStyle": "expanded"
            },
            "linting.collapsed": true,
            "debug.showErrorsInStatusBar": true,
            "brackets-git.gitPath": "git"
        }
    }
}